{
  "name": "@fe-monorepo/hook",
  "type": "module",
  "license": "MIT",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./hooks/*": {
      "types": "./dist/hooks/*.d.ts",
      "require": "./dist/hooks/*.cjs",
      "import": "./dist/hooks/*.js",
      "default": "./dist/hooks/*.js"
    }
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  }
}
